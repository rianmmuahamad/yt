{
  "version": 2,
  "builds": [
      {
          "src": "api/index.py",
          "use": "@vercel/python"
      },
      {
          "src": "*.html",
          "use": "@vercel/static"
      }
  ],
  "routes": [
      {
          "src": "/api/(.*)",
          "dest": "api/app.py"
      },
      {
          "src": "/downloads/(.*)",
          "dest": "api/app.py"
      },
      {
          "src": "/(.*)",
          "dest": "/$1"
      },
      {
          "src": "/",
          "dest": "/index.html"
      }
  ],
  "env": {
      "PYTHONUNBUFFERED": "1"
  }
}